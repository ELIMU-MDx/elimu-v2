{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.10",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "blade-ui-kit/blade-heroicons": "^2.0",
        "blade-ui-kit/blade-icons": "^1.2",
        "blade-ui-kit/blade-ui-kit": "^0.4",
        "chillerlan/php-qrcode": "^4.3",
        "doctrine/dbal": "^3.4",
        "guzzlehttp/guzzle": "^7.2",
        "hammerstone/airdrop": "^0.2.3",
        "kaydansky/correlation-coefficient": "^1.0",
        "kirschbaum-development/eloquent-power-joins": "^3",
        "laravel/framework": "^10",
        "laravel/horizon": "^5.10",
        "laravel/jetstream": "^3",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "masbug/flysystem-google-drive-ext": "^2.2",
        "predis/predis": "^2.0",
        "sentry/sentry-laravel": "^3.0",
        "spatie/browsershot": "^3.57",
        "spatie/data-transfer-object": "^3.3",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-data": "^3",
        "spatie/laravel-enum": "^3.0",
        "symfony/http-client": "^6.1",
        "symfony/mailgun-mailer": "^6.1"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^2.0",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/helo-laravel": "^1.4",
        "driftingly/rector-laravel": "^0.22.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7",
        "nunomaduro/larastan": "^2.2",
        "pestphp/pest": "^2",
        "pestphp/pest-plugin-laravel": "^2",
        "rector/rector": "^0.17.0",
        "spatie/laravel-ignition": "^2",
        "spatie/laravel-ray": "^1.31",
        "spatie/laravel-stubs": "^2.4",
        "spatie/pest-plugin-snapshots": "^2",
        "spatie/phpunit-snapshot-assertions": "^5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "src/Domain",
            "Support\\": "src/Support",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -N"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1.10"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
