{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.10",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "blade-ui-kit/blade-heroicons": "^1.3",
        "blade-ui-kit/blade-icons": "^1.2",
        "blade-ui-kit/blade-ui-kit": "^0.3",
        "guzzlehttp/guzzle": "^7.2",
        "honeybadger-io/honeybadger-laravel": "^3.13",
        "kirschbaum-development/eloquent-power-joins": "^2.6",
        "laravel/framework": "^9.33",
        "laravel/jetstream": "^2.6",
        "laravel/sanctum": "dev-develop",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "nunomaduro/phpinsights": "^2.0",
        "spatie/data-transfer-object": "^3.3",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-enum": "^3.0",
        "symfony/http-client": "^6.1",
        "symfony/mailgun-mailer": "^6.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/helo-laravel": "^1.4",
        "beyondcode/laravel-dump-server": "^1.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.2",
        "laravel/sail": "^1.15",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.3",
        "nunomaduro/larastan": "^0.7.11",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.4",
        "spatie/laravel-stubs": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App",
            "Domain\\": "src/Domain",
            "Support\\": "src/Support",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -N"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1.10"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
