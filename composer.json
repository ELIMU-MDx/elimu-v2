{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-simplexml": "*",
    "ext-zip": "*",
    "blade-ui-kit/blade-heroicons": "^1.2",
    "blade-ui-kit/blade-icons": "^1.1",
    "blade-ui-kit/blade-ui-kit": "^0.2.0",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "honeybadger-io/honeybadger-laravel": "^3.12",
    "kirschbaum-development/eloquent-power-joins": "^2.4",
    "laravel/framework": "^8.40",
    "laravel/jetstream": "^2.3",
    "laravel/sanctum": "^2.6",
    "laravel/tinker": "^2.5",
    "livewire/livewire": "^2.5",
    "maatwebsite/excel": "^3.1",
    "nunomaduro/phpinsights": "^2.0",
    "spatie/data-transfer-object": "^3.3",
    "spatie/laravel-activitylog": "^4.1",
    "spatie/laravel-enum": "^2.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.6",
    "barryvdh/laravel-ide-helper": "^2.10",
    "beyondcode/helo-laravel": "^1.3",
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "mouadziani/laravel-query-inspector": "^1.0",
    "nunomaduro/collision": "^5.0",
    "nunomaduro/larastan": "^0.7.11",
    "phpunit/phpunit": "^9.3.3",
    "spatie/laravel-stubs": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App",
      "Domain\\": "src/Domain",
      "Support\\": "src/Support",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "8.0"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
